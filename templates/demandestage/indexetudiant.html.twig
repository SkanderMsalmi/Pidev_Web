{% extends 'base.html.twig' %}

{% block title %}Demandestage Recruteur{% endblock %}

{% block body %}
    <h1 style="padding:80px">Demandes Stages </h1>

    <table class="table">
        <thead>
            <tr>
                
                <th>Etat</th>
                <th>Sujet Stage</th>
                <th>Nom&Pr√®nom Etudiant</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for demandestage in demandestages %}
            <tr>
                
                <td>{{ demandestage.etat }}</td>
                <td> {{ demandestage.idstage}}</td>
                <td>{{ demandestage.iduser }}</td>
                <td>
                    <a href="{{ path('app_demandestage_showe', {'iddemande': demandestage.iddemande}) }}">show</a>
                  
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
