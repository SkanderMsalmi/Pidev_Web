{% extends 'baseAdmin.html.twig' %}

{% block title %}Entretiens {% endblock %}

{% block body %}
    <main id="main" class="main">

    <h1 style="padding:80px">Entretiens</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Identretien</th>
                <th>Dateentretien</th>
                <th>Stage</th>
                <th>Etudiant</th>
                <th>Lienentretien</th>
               
            </tr>
        </thead>
        <tbody>
        {% for entretien in entretiens %}
            <tr>
                <td>{{ entretien.identretien }}</td>
                <td>{{ entretien.dateentretien | date ('Y-m-d H:i:s') }}</td>
                <td>{{ entretien.idstage }}</td>
                <td>{{ entretien.iduser }}</td>
                <td>{{ entretien.lienentretien }}</td>
                
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
    </main>

    
{% endblock %}
