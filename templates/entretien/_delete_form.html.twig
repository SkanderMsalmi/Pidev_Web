<form method="post" action="{{ path('app_entretien_delete', {'identretien': entretien.identretien}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ entretien.identretien) }}">
    <button class="btn">Delete</button>
</form>
